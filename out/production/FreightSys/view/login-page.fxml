   <children>
      <TextField fx:id="loginField" layoutX="33.0" layoutY="44.0" promptText="Login:" />
      <PasswordField fx:id="passwordField" layoutX="33.0" layoutY="85.0" promptText="Password:" />
      <Button layoutX="33.0" layoutY="128.0" mnemonicParsing="false" onAction="#validate" text="Log In" />
      <Button layoutX="130.0" layoutY="128.0" mnemonicParsing="false" onAction="#register" text="Register" />
   </children>
</AnchorPane>
